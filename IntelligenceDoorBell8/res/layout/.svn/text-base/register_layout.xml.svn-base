<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    >
    <RelativeLayout
               android:layout_width="500.0dip"
               android:layout_height="50.0dip"
               android:background="@drawable/title">
       <ImageButton     
                      android:id="@+id/fanhui"
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:layout_marginLeft="13.0dip"
                      android:layout_marginTop="10.0dip"
                      android:background="@drawable/back"/>
       <TextView    
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20.0sp"
                android:layout_marginLeft="116.0dip"
                android:layout_marginTop="13.0dip"              
                android:text="用户注册"
                android:textColor="#FFFFFFFF"/>  
         <TextView   
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" 
                 android:textSize="13.0sp"
                 android:id="@+id/ok"
                 android:text="确定"
                 android:textColor="#FFFFFFFF"  
                 android:layout_marginLeft="260.0dip"
                 android:layout_marginTop="17.5dip"/>                          
        </RelativeLayout>
    
     <LinearLayout  
               android:layout_width="290.0dip"
               android:layout_height="45.0dip"
               android:layout_marginTop="25.0dip"
               android:layout_marginLeft="15.0dip"
               android:orientation="horizontal"
               android:background="@drawable/whitebackground">
         <TextView   
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_marginLeft="15.0dip"
                android:layout_marginTop="6.5dip"
               
               android:text="手机号 :"/>
         <EditText   
                   android:id="@+id/tel"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginLeft="8.0dip"
                   android:layout_marginTop="6.0dip"
                   android:textSize="20sp"
                   android:singleLine="true"
                   android:background="@null"
                   
                   android:hint="必填" />
      </LinearLayout>
      
     <ImageView    
            android:layout_width="290.0dip"
               android:layout_height="wrap_content"
               android:layout_marginLeft="15.0dip"
               android:background="@drawable/buttonangang"    />
     <RelativeLayout   
               android:layout_width="290.0dip"
               android:layout_height="45.0dip"
              
               android:layout_marginLeft="15.0dip"
               android:layout_marginTop="25.0dip"
               android:background="@drawable/whitebackground" >
        <TextView   
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_marginLeft="15.0dip"
                android:layout_marginTop="6.0dip"
                android:text="验证码 :"/>
         <EditText   
                   android:id="@+id/yanzheng"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginLeft="90.0dip"
                   android:layout_marginTop="6.0dip"
                   android:singleLine="true"
                   android:background="@null"
                   android:maxLength="9"
                   android:textSize="20.0sp"
                   android:hint="必填" />
         
         <Button  
                  android:layout_alignParentRight="true"
                   android:id="@+id/switchphoto"
                   android:layout_width="50.0dip"
                   android:layout_height="40.0dip"
                   android:layout_marginLeft="100.0dip"
                   android:layout_marginTop="6.0dip"
                   android:background="@drawable/yanzheng"
                   
                   />
        </RelativeLayout>
       
        <ImageView    
            android:layout_width="290.0dip"
               android:layout_height="wrap_content"
               android:layout_marginLeft="15.0dip"
               android:background="@drawable/buttonangang"    />
        
         <LinearLayout   
               android:layout_width="290.0dip"
               android:layout_height="45.0dip"
               android:orientation="horizontal"
               android:layout_marginLeft="15.0dip"
               android:background="@drawable/whitebackground" >
        <TextView   
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_marginLeft="15.0dip"
                android:layout_marginTop="6.0dip"
                android:text="昵     称:"/>
         <EditText   
                   android:id="@+id/nickname"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginLeft="8.0dip"
                   android:layout_marginTop="6.0dip"
                   android:singleLine="true"
                   android:background="@null"
                   android:textSize="20.0sp"
                   android:hint="必填" />  
       </LinearLayout>         
     
         <ImageView    
            android:layout_width="290.0dip"
               android:layout_height="wrap_content"
               android:layout_marginLeft="15.0dip"
               android:background="@drawable/buttonangang"    />
        
         <LinearLayout   
               android:layout_width="290.0dip"
               android:layout_height="45.0dip"
               android:orientation="horizontal"
               android:layout_marginLeft="15.0dip"
               android:background="@drawable/whitebackground" >
        <TextView   
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_marginLeft="15.0dip"
                android:layout_marginTop="6.0dip"
                android:text="密     码:"/>
         <EditText   
                   android:id="@+id/newpass"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginLeft="8.0dip"
                   android:layout_marginTop="6.0dip"
                   android:singleLine="true"
                   android:background="@null"
                   android:textSize="20.0sp"
                  
                   android:hint="必填" />  
       </LinearLayout>         
     
      <ImageView    
            android:layout_width="290.0dip"
               android:layout_height="wrap_content"
               android:layout_marginLeft="15.0dip"
               android:background="@drawable/buttonangang"    />
        
         <LinearLayout   
               android:layout_width="290.0dip"
               android:layout_height="45.0dip"
               android:orientation="horizontal"
               android:layout_marginLeft="15.0dip"
               android:background="@drawable/whitebackground" >
        <TextView   
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_marginLeft="11.0dip"
                android:layout_marginTop="6.0dip"
                android:text="确认密码:"/>
         <EditText   
                   android:id="@+id/confimpass"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginLeft="8.0dip"
                   android:layout_marginTop="6.0dip"
                   android:singleLine="true"
                   android:background="@null"
                   android:textSize="20.0sp"
                  
                   android:hint="必填" />  
       </LinearLayout>         
     
    

</LinearLayout>
